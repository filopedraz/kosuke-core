{
  "name": "mobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "ios:dev": "APP_VARIANT=development expo start --ios",
    "ios:prod": "APP_VARIANT=production expo start --ios",
    "ios:run:dev": "APP_VARIANT=development expo run:ios",
    "ios:run:prod": "APP_VARIANT=production expo run:ios",
    "ios:build:dev": "eas build --profile development --platform ios",
    "ios:build:prod": "eas build --profile production --platform ios",
    "ios:build:prod:local": "eas build --profile production --non-interactive --platform ios --local",
    "clean:cache": "rm -rf node_modules package-lock.json .cache .expo ios && npm cache clean --force && xcrun simctl shutdown all && xcrun simctl erase all",
    "web": "expo start --web",
    "lint": "expo lint",
    "ios": "expo run:ios",
    "ios:release:testflight": "eas build --platform ios --profile production --non-interactive --auto-submit",
    "android": "expo run:android"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.15.0",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-menu/menu": "^2.0.0",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/elements": "^2.6.4",
    "@react-navigation/native": "^7.1.17",
    "@sentry/react-native": "~7.1.0",
    "@tanstack/react-query": "^5.89.0",
    "expo": "~54.0.9",
    "expo-auth-session": "~7.0.8",
    "expo-blur": "~15.0.7",
    "expo-constants": "~18.0.9",
    "expo-crypto": "^15.0.7",
    "expo-dev-client": "~6.0.12",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-insights": "~0.10.7",
    "expo-linking": "~8.0.8",
    "expo-random": "^14.0.1",
    "expo-router": "~6.0.7",
    "expo-secure-store": "~15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.7",
    "lucide-react-native": "^0.544.0",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.13.0",
    "react-native-web": "~0.21.1",
    "react-native-webview": "13.16.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@types/react": "~19.1.13",
    "eslint": "^9.35.0",
    "eslint-config-expo": "~10.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.2"
  },
  "private": true
}
